{
    "name": "inweb/payment",
    "description": "Payments manipulation",
    "keywords": [
        "framework",
        "laravel",
        "inweb",
        "payment"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "inweb/base": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Payment\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Payment\\Tests\\": "tests/",
            "InWeb\\Payment\\Database\\Seeds\\": "src/database/seeds/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Payment\\PaymentServiceProvider"
            ]
        }
    }
}
